<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.sharding.jdbc.demo.dao.UserRepository">
    <select id="getAll" resultType="com.example.sharding.jdbc.demo.model.User" >
        SELECT
        id id,
        nickname nickName,
        avatar avatar
        FROM
        users
    </select>
</mapper>